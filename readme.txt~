ssh-keygen -t rsa -C "youremail@example.com"
home/.ssh
id_rsa      私匙
id_rsa.pub  公匙

git init <dir>

git add <file1 file2 file3>
git commit -m <message>

git status       工作区变更
git diff <file>  显示变更

git log --pretty=oneline

wait for reset

git reset --hard HEAD^ HEAD^^ HEAD~100  返回之前的版本

cat <file>       查看文件

git reflog       命令记录


